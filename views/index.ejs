<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <a href="/new">Add Message</a>
    <% messages.forEach((message) => { %>
      <h2>Message</h2>
      <p>Text: <%= message.text %></p>
      <p>User: <%= message.user %></p>
      <p>Added: <%= message.added %></p>
    <% }); %>
  </body>
</html>
